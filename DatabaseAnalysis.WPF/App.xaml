<Application x:Class="DatabaseAnalysis.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DatabaseAnalysis.WPF"
             xmlns:v1="clr-namespace:DatabaseAnalysis.WPF.MVVM.Views.FormViews"
             xmlns:vm1="clr-namespace:DatabaseAnalysis.WPF.MVVM.ViewModels.FormViewModel"
             xmlns:v="clr-namespace:DatabaseAnalysis.WPF.MVVM.Views"
             xmlns:vm="clr-namespace:DatabaseAnalysis.WPF.MVVM.ViewModels">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type vm:AnnualReportsViewModel}">
                <v:AnnualReportsControl/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:OperReportsViewModel}">
                <v:OperReportsControl/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:FormsViewModel}">
                <v:FormView/>
            </DataTemplate>

            <DataTemplate DataType="{x:Type vm1:Form11ViewModel}">
                <v1:Form_1_1/>
            </DataTemplate>
            
        </ResourceDictionary>
    </Application.Resources>
</Application>
